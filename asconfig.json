{
  "entries": [
    "as/assembly.ts"
  ],
  "options": {
    "outFile": "src/wasm/assembly.wasm",
    "textFile": "src/wasm/assembly.wat",
    "sourceMap": true,
    "importMemory": true,
    "sharedMemory": true,
    "maximumMemory": 1024,
    "enable": ["threads"]
  },
  "targets": {
    "debug": {
      "debug": true
    },
    "release": {
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "converge": false,
      "noAssert": false
    }
  }
}
